{
  "private": true,
  "name": "@substreams/node",
  "version": "0.0.1",
  "description": "Substreams Node.js",
  "license": "MIT",
  "repository": "substreams-js/substreams-node",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "substreams-sink-prometheus": "dist/index.js"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc",
    "pkg": "pkg --target linux-x64,mac-x64,win-x64,mac-arm64 --output substreams-node dist/index.js",
    "prepublishOnly": "npm run test && npm run build"
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "keywords": [
    "substreams",
    "streamingfast",
    "thegraph",
    "graph"
  ],
  "dependencies": {
    "@bufbuild/connect-node": "latest",
    "@substreams/core": "0.0.x"
  },
  "devDependencies": {
    "@tsconfig/recommended": "latest",
    "@types/node": "latest",
    "pkg": "latest",
    "typescript": "latest"
  }
}
